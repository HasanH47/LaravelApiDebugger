{
    "name": "hasanh47/laravel-api-debugger",
    "description": "A simple API debugger for Laravel",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "symfony/http-foundation": "^7.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hasanh47\\LaravelApiDebugger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "HasanH47",
            "email": "hasan@hasanh47.my.id"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "HasanH47\\LaravelApiDebugger\\ApiDebuggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
